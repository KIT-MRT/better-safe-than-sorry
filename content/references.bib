@article{zieglerMakingBerthaDrive2014,
  title = {Making {{Bertha Drive}} – {{An Autonomous Journey}} on a {{Historic Route}}},
  author = {Ziegler, J. and Bender, P. and Schreiber, M. and Lategahn, H. and Strauss, T. and Stiller, C. and Dang, T. and Franke, U. and Appenrodt, N. and Keller, C. G. and Kaus, E. and Herrtwich, R. G. and Rabe, C. and Pfeiffer, D. and Lindner, F. and Stein, F. and Erbs, F. and Enzweiler, M. and Knoppel, C. and Hipp, J. and Haueis, M. and Trepte, M. and Brenk, C. and Tamke, A. and Ghanaat, M. and Braun, M. and Joos, A. and Fritz, H. and Mock, H. and Hein, M. and Zeeb, E.},
