@inproceedings{poggenhansLanelet2HighdefinitionMap2018,
  title = {Lanelet2: {{A}} High-Definition Map Framework for the Future of Automated Driving},
  shorttitle = {Lanelet2},
  booktitle = {International {{Conference}} on {{Intelligent Transportation Systems}}},
  author = {Poggenhans, Fabian and Pauls, Jan-Hendrik and Janosovits, Johannes and Orf, Stefan and Naumann, Maximilian and Kuhnt, Florian and Mayr, Matthias},
  date = {2018-11},
  pages = {1672--1679},
  publisher = {IEEE},
  location = {Maui, Hawaii, USA},
  issn = {2153-0009},
  doi = {10.1109/ITSC.2018.8569929},
  abstract = {Although accurate and comprehensive maps are indispensable for highly automated driving, especially in complex urban scenarios, there are hardly any publications in which requirements for these maps are discussed. In our opinion, such maps must meet high demands in terms of accuracy, completeness, verifiability and extensibility, so that the resulting complexity can only be handled by an enclosing, carefully designed software framework. In this paper we therefore introduce the open-source map framework Lanelet2 implemented in C++ and explain the underlying concept. The goal of Lanelet2 is not only to be usable for typical, isolated applications such as localization or motion planning, but for various potential applications of maps for highly automated driving. On the basis of both abstract and real examples we show the concrete structure of Lanelet2 maps and its use for automated driving.},
  keywords = {accurate maps,C++,complex urban scenarios,Complexity theory,comprehensive maps,computational complexity,driver information systems,geographic information systems,high-definition map framework,highly automated driving,IV2020 Quelle,Lanelet2 maps,open-source map framework Lanelet2,Planning,Roads,Routing,Sensors,Software,software architecture,software framework,traffic engineering computing,Urban areas},
}

@inproceedings{orzechowskiTacklingOcclusionsLimited2018,
  title = {Tackling {{Occlusions}} \& {{Limited Sensor Range}} with {{Set-based Safety Verification}}},
  booktitle = {International {{Conference}} on {{Intelligent Transportation Systems}}},
  author = {Orzechowski, Piotr F. and Meyer, Annika and Lauer, Martin},
  date = {2018-11},
  pages = {1729--1736},
  publisher = {IEEE},
  location = {Maui, HI, USA},
  doi = {10.1109/ITSC.2018.8569332},
  abstract = {Provable safety is one of the most critical challenges in automated driving. The behavior of numerous traffic participants in a scene cannot be predicted reliably due to complex interdependencies and the indiscriminate behavior of humans. Additionally, we face high uncertainties and only incomplete environment knowledge. Recent approaches minimize risk with probabilistic and machine learning methods - even under occlusions. These generate comfortable behavior with good traffic flow, but cannot guarantee safety of their maneuvers. Therefore, we contribute a safety verification method for trajectories under occlusions. The field-of-view of the ego vehicle and a map are used to identify critical sensing field edges, each representing a potentially hidden obstacle. The state of occluded obstacles is unknown, but can be over-approximated by intervals over all possible states. Then set-based methods are extended to provide occupancy predictions for obstacles with state intervals. The proposed method can verify the safety of given trajectories (e.g. if they ensure collision-free fail-safe maneuver options) w.r.t. arbitrary safe-state formulations. The potential for provably safe trajectory planning is shown in three evaluative scenarios.},
  keywords = {2019 RS+RSS Quellen,ADAS,automated driving,automated vehicles,collision avoidance,collision-free fail-safe maneuver options,comfortable behavior,complex interdependencies,critical sensing field edges,ego vehicle,field-of-view,formal verification,given trajectories,good traffic flow,incomplete environment knowledge,indiscriminate behavior,IV2020 Quelle,learning (artificial intelligence),limited sensor range,mobile robots,multi-robot systems,object detection,occluded obstacles,occlusions,occupancy predictions,Planning,potentially hidden obstacle,probabilistic machine,provable safety,provably safe trajectory planning,reachability analysis,risk assessment,Risk management,road traffic,Robot sensing systems,safe-state formulations,Safety,safety verification method,set-based methods,set-based safety verification,state intervals,traffic engineering computing,traffic participants,Trajectory,Uncertainty},
}

