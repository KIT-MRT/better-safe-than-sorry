@inproceedings{werlingAutomaticCollisionAvoidance2012,
  title = {Automatic Collision Avoidance Using Model-Predictive Online Optimization},
  booktitle = {{{IEEE Conference}} on {{Decision}} and {{Control}}},
  author = {Werling, Moritz and Liccardo, Darren},
  date = {2012-12},
