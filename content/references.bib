@unpublished{shalev-shwartzFormalModelSafe2017,
  title = {On a {{Formal Model}} of {{Safe}} and {{Scalable Self-driving Cars}}},
  author = {Shalev-Shwartz, Shai and Shammah, Shaked and Shashua, Amnon},
  date = {2017-08-21},
  eprint = {1708.06374},
