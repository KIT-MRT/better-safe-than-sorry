}

@inproceedings{hubmannBeliefStatePlanner2018,
  title = {A {{Belief State Planner}} for {{Interactive Merge Maneuvers}} in {{Congested Traffic}}},
  booktitle = {International {{Conference}} on {{Intelligent Transportation Systems}}},
  author = {Hubmann, Constantin and Schulz, Jens and Xu, Gavin and Althoff, Daniel and Stiller, Christoph},
  date = {2018-11},
