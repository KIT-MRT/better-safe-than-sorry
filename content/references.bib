  keywords = {2019 RS+RSS Quellen,Benchmark testing,collision avoidance,CommonRoad Website,composable benchmarks,control engineering computing,cost function,Cost function,dangerous situations,ITSC18 Occlusions Quellen,Libraries,Mathematical model,motion planning,Planning,road network,road traffic,road vehicles,Roads,scientific publications,static obstacles,traffic engineering computing,traffic situations,vehicle dynamics,Vehicle dynamics},
