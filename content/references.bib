@article{bengler_three_2014,
  title = {Three {{Decades}} of {{Driver Assistance Systems}}: {{Review}} and {{Future Perspectives}}},
  shorttitle = {Three {{Decades}} of {{Driver Assistance Systems}}},
  author = {Bengler, K. and Dietmayer, K. and Farber, B. and Maurer, M. and Stiller, C. and Winner, H.},
  date = {2014-10-27},
  journaltitle = {IEEE Intelligent Transportation Systems Magazine},
  volume = {6},
