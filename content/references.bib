  booktitle = {Augmented {{Reality}} and {{Virtual Reality}}: {{The Power}} of {{AR}} and {{VR}} for {{Business}}},
  author = {Stadler, Sebastian and Cornet, Henriette and Novaes Theoto, Tatiana and Frenkler, Fritz},
