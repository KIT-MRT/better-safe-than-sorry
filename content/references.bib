@article{rasouliAutonomousVehiclesThat2020,
  title = {Autonomous {{Vehicles That Interact With Pedestrians}}: {{A Survey}} of {{Theory}} and {{Practice}}},
  shorttitle = {Autonomous {{Vehicles That Interact With Pedestrians}}},
  author = {Rasouli, A. and Tsotsos, J. K.},
